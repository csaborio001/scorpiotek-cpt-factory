    "type": "library",
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "8",
        "squizlabs/php_codesniffer": "^3.4",
        "phpdocumentor/reflection-docblock": "^4.3"
    },
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Christian Saborio",
            "email": "csaborio@scorpiotek.com"
        }
    ],
    "autoload": {
        "psr-4": {
